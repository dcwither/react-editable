{
  "name": "react-editable-decorator",
  "version": "0.2.0",
  "description": "Decorator for maintaining editing state of React components",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcwither/react-editable-decorator.git"
  },
  "author": "Devin Witherspoon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dcwither/react-editable-decorator/issues"
  },
  "homepage": "https://github.com/dcwither/react-editable-decorator",
  "keywords": [
    "react-component",
    "react"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:dist": "tsc -project .",
    "build:storybook": "build-storybook",
    "clean:dist": "rimraf dist",
    "deploy:storybook": "storybook-to-ghpages",
    "lint": "tslint {src,examples,stories}/{**/*,*}.{ts,tsx,js,jsx}",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run clean:dist && npm run build:dist",
    "release": "np --no-cleanup",
    "start": "start-storybook -p 6006",
    "test": "jest --config jest.config.json --verbose",
    "test:ci": "npm run test -- --ci --coverage && cat ./coverage/lcov.info | coveralls",
    "test:watch": "npm run test -- --watch"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx,ts,json,tsx,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.3",
    "@storybook/addon-links": "^3.4.3",
    "@storybook/react": "^3.4.3",
    "@storybook/storybook-deployer": "^2.3.0",
    "@testing-library/react": "^10.0.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^25.1.4",
    "@types/prop-types": "^15.7.3",
    "@types/ramda": "^0.25.24",
    "@types/react": "^16.9.17",
    "@types/recompose": "^0.26.1",
    "@types/testing-library__react-hooks": "^3.2.0",
    "babel-cli": "^6.6.4",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.0",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "immutability-helper": "^2.7.0",
    "jest": "^25.1.0",
    "lint-staged": "^7.0.5",
    "material-ui": "^1.0.0-beta.44",
    "nodemon": "^1.17.3",
    "np": "^2.20.1",
    "prettier": "^1.19.1",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "raw-loader": "^0.5.1",
    "react": "^16.12.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.13.0",
    "recompose": "^0.27.0",
    "rimraf": "^2.6.2",
    "storybook-readme": "^3.3.0",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-eslint-rules": "^5.1.0",
    "tslint-react": "^3.5.1",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "prop-types": "^15.5.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "invariant": "^2.2.4"
  }
}
