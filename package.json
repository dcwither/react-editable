{
  "name": "@dcwither/react-editable",
  "version": "0.3.1",
  "description": "State wrapper for controlled components managing presenting, editing, committing state transitions.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcwither/react-editable.git"
  },
  "author": "Devin Witherspoon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dcwither/react-editable/issues"
  },
  "homepage": "https://github.com/dcwither/react-editable",
  "keywords": [
    "react-component",
    "react"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:dist": "tsc -project .",
    "build:storybook": "build-storybook",
    "clean:dist": "rimraf dist",
    "clean:storybook": "rimraf storybook-static",
    "lint": "eslint --ext=js,jsx,ts,tsx .",
    "prepublishOnly": "npm run clean:dist && npm run build:dist",
    "release": "np --no-cleanup",
    "start": "start-storybook -p 6006",
    "test": "jest --config jest.config.json --verbose",
    "test:ci": "npm run test -- --ci --coverage && cat ./coverage/lcov.info | codecov",
    "test:watch": "npm run test -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --ext=js,jsx,ts,tsx --fix",
      "git add"
    ],
    "*.{js,jsx,ts,json,tsx,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt-cli": "^8.3.5",
    "@material-ui/core": "^4.9.7",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/preset-typescript": "^2.1.0",
    "@storybook/react": "^5.3.17",
    "@testing-library/react": "^10.0.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.25",
    "@types/testing-library__react-hooks": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-preset-stage-2": "^6.5.0",
    "bluebird": "^3.5.0",
    "codecov": "^3.6.5",
    "css-loader": "^0.28.11",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^1.1.11",
    "husky": "^4.2.3",
    "immer": "^6.0.2",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.9",
    "np": "^2.20.1",
    "prettier": "^1.19.1",
    "prop-types": "^15.6.1",
    "raw-loader": "^4.0.2",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.7.1",
    "react-dom": "^16.13.0",
    "react-test-renderer": "^16.13.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "invariant": "^2.2.4"
  }
}
